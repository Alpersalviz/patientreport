    <![endif]-->

</head>

<body>
{% block login %}
{% endblock %}
<div id="wrapper">
    {% if app.session.get('name') != null %}
    <!-- Navigation -->
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="index.html">MAS / Admin</a>
        </div>
        <!-- Top Menu Items -->
        <ul class="nav navbar-right top-nav">
            <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> Active Admin <b class="caret"></b></a>
                <ul class="dropdown-menu">

                    <li class="divider"></li>
                    <li>
                        <a href="{{ url('logout') }}"><i class="fa fa-fw fa-power-off"></i> Log Out</a>
                    </li>
                </ul>
            </li>
        </ul>
        <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
        <div class="collapse navbar-collapse navbar-ex1-collapse">
            <ul class="nav navbar-nav side-nav">
                <li class="active">
                    <a href="{{ url('homepage') }}"><i class="fa fa-fw fa-dashboard"></i> Home</a>
                </li>
                <li>
                    <a href="javascript:;" data-toggle="collapse" data-target="#hospital"><i class="fa fa-fw fa-arrows-v"></i> Hospital <i class="fa fa-fw fa-caret-down"></i></a>
                    <ul id="hospital" class="collapse">

                        <li>
                            <a href="{{ url('hospital_add') }}">Add Hospital</a>

                        </li>

                        <li>
                            <a href="{{ url('hospital_list') }}">List Hospital</a>
                        </li>


                    </ul>
                </li>
                <li>
                    <a href="javascript:;" data-toggle="collapse" data-target="#hasta"><i class="fa fa-fw fa-arrows-v"></i> Patient <i class="fa fa-fw fa-caret-down"></i></a>
                    <ul id="hasta" class="collapse">

                        <li>
                            <a href="{{ url('patient_add') }}">Add Patient</a>
                        </li>

                        <li>
                            <a href="{{ url('patient_list',{"firstmonth" : 0,'lastmonth':0,'year':0}) }}">List Patient</a>
                        </li>

                    </ul>
                </li>


                <li>
                    <a href="javascript:;" data-toggle="collapse" data-target="#report"><i class="fa fa-fw fa-arrows-v"></i> Implant Report <i class="fa fa-fw fa-caret-down"></i></a>
                    <ul id="report" class="collapse">

                        <li>
                            <a href="{{ url('year_report',{"year" : 2017}) }}">Year Report</a>
                        </li>

                        <li>
                            <a href="{{ url('month_report',{"year" : 2017,'month':01}) }}">Month Report</a>
                        </li>

                    </ul>
                </li>

                <li>
                    <a href="javascript:;" data-toggle="collapse" data-target="#competitor"><i class="fa fa-fw fa-arrows-v"></i> Competitor <i class="fa fa-fw fa-caret-down"></i></a>
                    <ul id="competitor" class="collapse">

                        <li>
                            <a href="{{ url('competitor_add') }}">Competitor Add</a>
                        </li>

                        <li>
                            <a href="{{ url('month_report',{"year" : 2017,'month':01}) }}">Competitor Report</a>
                        </li>

                    </ul>
                </li>




            </ul>
        </div>
        <!-- /.navbar-collapse -->
    </nav>

    <div id="page-wrapper">


        <div class="container-fluid">

            <!-- Page Heading -->
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header" style="color:#880000">
                        MAS <small style="color:#080808">Medical Products</small>
                    </h1>
                    <ol class="breadcrumb base">
                        <li class="active">
                            <i class="fa fa-dashboard"></i> ADMINISTRATOR PANEL
                        </li>
                    </ol>
                </div>
            </div>
            <!-- /.row -->
            {% block content %}
            {% endblock %}

        </div>
        <!-- /.container-fluid -->

    </div>
    <!-- /#page-wrapper -->
{% endif %}
</div>
<!-- /#wrapper -->

<!-- jQuery -->
<script src="{{ asset('static/js/jquery.js') }}"></script>

<!-- Bootstrap Core JavaScript -->
<script src="{{ asset('static/js/bootstrap.min.js') }}"></script>

<script type="text/javascript" src="{{ asset('static/js/bootstrap-datetimepicker.js') }}" charset="UTF-8"></script>
<script type="text/javascript" src="{{ asset('static/js/jquery.twbsPagination.min.js') }}" charset="UTF-8"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.6/moment.min.js"></script>
