    <title>Mas Admin</title>
    {% set version = '?v=0.0.4' %}

    <!-- Bootstrap Core CSS -->
    <link href="{{ asset('static/css/bootstrap.min.css') }}{{ version }}" rel="stylesheet">

    <link href="{{ asset('static/css/admin.css') }}{{ version }}" rel="stylesheet" type="text/css"/>
    <!-- Custom CSS -->
    <link href="{{ asset('static/css/sb-admin.css') }}{{ version }}" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.37/css/bootstrap-datetimepicker.min.css" />
    <!-- Morris Charts CSS -->
    <link href="{{ asset('static/css/plugins/morris.css') }}{{ version }}" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="{{ asset('static/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css">

    <link href="{{  asset('static/css/bootstrap-datetimepicker.min.css') }}" rel="stylesheet" media="screen">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.15/css/dataTables.bootstrap.min.css" />
    <link rel="stylesheet" href=" https://cdn.datatables.net/responsive/2.1.1/css/responsive.bootstrap.min.css" />




    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
